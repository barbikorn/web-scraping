    <script>


        setTimeout(function () {
            var projnext = $(".projnext").attr('onclick');
            var projfavi = $(".projfavi").attr('onclick');
            var projrec = $(".projrec").attr('onclick');
            var sha01 = $(".sha01").attr('onclick');
            var shl01 = $(".shl01").attr('onclick');

            if (projnext != undefined && projnext != "" && projnext != null) {
                $(".projnext3").css('display', 'inline-block');
            } else {
                $('.sentmail03').hide();
            }
            if (projfavi != undefined && projfavi != "" && projfavi != null) {
                $(".projfavi3").css('display', 'inline-block');
            } else {
                $('.sentmail03').hide();
            }
            if (projrec != undefined && projrec != "" && projrec != null) {
                $(".projrec3").css('display', 'inline-block');
            } else {
                $('.sentmail03').hide();
            }
            if (sha01 != undefined && sha01 != "" && sha01 != null) {
                $(".sha03").css('display', 'inline-block');
            }
            //console.log(projnext, projfavi, sha01, shl01);
            if (shl01 != undefined && shl01 != "" && shl01 != null) {
                $(".shl03").css('display', 'inline-block');
            }

        }, 3000);
        setTimeout(function () {
            var projnext = $(".projnext").attr('onclick');
            var projfavi = $(".projfavi").attr('onclick');
            var projrec = $(".projrec").attr('onclick');
            var sha01 = $(".sha01").attr('onclick');
            var shl01 = $(".shl01").attr('onclick');

            if (projnext != undefined && projnext != "" && projnext != null) {
                $(".projnext3").css('display', 'inline-block');
            } else {
                $('.sentmail03').hide();
            }
            if (projfavi != undefined && projfavi != "" && projfavi != null) {
