      <input type="hidden" id="hdack" value="">
    <script type="text/javascript" src="/Scripts/hda-script-v4.min.js"></script>

    <script type="text/javascript" >
        var objLogs = {
            sID: 18,
            mtID: 1801,
            parameterValues: escapeSpecChar(''),
            hdaModel: 3,
            hdaID: '10461',
            hdaName: 'Developer',
            hdaLocation: '',
            hdaPrj: '',
            hdaDev: '',
            hdaPrice: '',
            hdaType: 'Info',
            hdaDesc: escapeSpecChar('บริษัท ควอลิตี้ เฮ้าส์ จำกัด (มหาชน)'),
            ProcMode: 1
        };
    </script>
    <script type="text/javascript">
        
        var getusernameHDA = function () {

            var data = ('') != '' ? ('') : '';
            return data
        }
         var HDAobjLogs = {};
         if (typeof (objLogs) !== 'undefined') {
             var objUser = {
                 userCode: hdaUserAccount(),
                 userType: 'CK',
                 userName: '',
                 requestedDevice: 'D'
             };
             HDAobjLogs = $.extend(objLogs, objUser);
             hdaPostData(HDAobjLogs);
             $('#hdack').val(HDAobjLogs.userCode);
         }
         var HDAClick = function (t, n, l, m, d) {

             if (t != '' && t != null) { HDAobjLogs['hdaType'] = t; }
             if (n != '' && n != null) { HDAobjLogs['hdaName'] = n; }
             if (l != '' && l != null) { HDAobjLogs['hdaLocation'] = l; }
             if (m != '' && m != null) { HDAobjLogs['hdaModel'] = m; }
             if (d != '' && d != null) { HDAobjLogs['hdaDesc'] = escapeSpecChar(d); }
             HDAobjLogs['userName'] = getusernameHDA();
             hdaPostData(HDAobjLogs);
         }
         var HDAClickPage = function (m, n, t, d) {

             if (m != '' && m != null) { HDAobjLogs['mtID'] = m; }
             if (n != '' && n != null) { HDAobjLogs['hdaName'] = n; }
             if (t != '' && t != null) { HDAobjLogs['hdaType'] = t; }
             if (d != '' && d != null) { HDAobjLogs['hdaDesc'] = escapeSpecChar(d); }
             HDAobjLogs['userName'] = getusernameHDA();
             ////console.log(HDAobjLogs);
             hdaPostData(HDAobjLogs);
         }
         var HDASendMail = function (p) {
             HDAobjLogs['hdaType'] = 'sendmail';
             if (p != '' && p != null) { HDAobjLogs['parameterValues'] = escapeSpecChar(p); }
             HDAobjLogs['userName'] = getusernameHDA();
             ////console.log(HDAobjLogs);
             hdaPostData(HDAobjLogs);
         }
         var HDAClickPage = function (m, n, t, d) {

             if (m != '' && m != null) { HDAobjLogs['mtID'] = m; }
             if (n != '' && n != null) { HDAobjLogs['hdaName'] = n; }
             if (t != '' && t != null) { HDAobjLogs['hdaType'] = t; }
             if (d != '' && d != null) { HDAobjLogs['hdaDesc'] = escapeSpecChar(d); }
             HDAobjLogs['userName'] = getusernameHDA();
             hdaPostData(HDAobjLogs);
         }
         $('#home_price').click(function () {
             HDAClick('mort-cal', '', '', '', 'ราคาบ้าน');
         })
         $('#down_payment').click(function () {
             HDAClick('mort-cal', '', '', '', 'เงินดาวน์|บาท');
         })
         $('#down_payment_percentage').click(function () {
             HDAClick('mort-cal', '', '', '', 'เงินดาวน์|ร้อยละ');
         })
         $('#nloan,#interest_rate').click(function () {
             HDAClick('mort-cal', '', '', '', 'อัตราดอกเบี้ย');
         })
         $('#loan_term').click(function () {
             HDAClick('mort-cal', '', '', '', 'ระยะเวลากู้|ร้อยละ');
         })
         

    </script>
    <!--+++++++++++++++++++++    HDA End ++++++++++++++++++++++++-->
    <script>
        function adssys(id, url, page) {
            $.getJSON('https://ipapi.co/json/', function (data) {
                ip = (data.ip);
                $.post('/adssystem/api/AdsLogAPI', { C_Word: id, RequestedSource: 'D', RequestedIP: ip, UserAgent: HDAobjLogs.userCode, C_Pagename: page }, function (data) {
                    if (data == '' || data == null) {
                        //console.log(data);
                    }
                });
            });


        }


        $.each($('a[href*="/adssystem/UpdateClicks/"]'), function (i, v) {
            var AdsSys = ($(v).attr("href").replace('/adssystem/UpdateClicks/', ''));
            $.get('/adssystem/api/AdsLogAPI', { C_Word: AdsSys }, function (data) {
                var path2 = '/d/qh';
                var path3 = 'd';
                var C_PAGE_NAME = data.C_PAGE_NAME;
                C_PAGE_NAME = (path3 == 'review' ? 'V9_REVIEW' : C_PAGE_NAME);
                C_PAGE_NAME = (path2 == '/home' ? 'V9_HOME' : C_PAGE_NAME);
                C_PAGE_NAME = (path2 == '/condo' ? 'V9_CONDO' : C_PAGE_NAME);
                C_PAGE_NAME = (path3 == 'hometips' ? 'V8_HOMETIPS' : C_PAGE_NAME);
                C_PAGE_NAME = (path3 == 'tv' ? 'V9_TV' : C_PAGE_NAME);
                $(v).attr("href", "javascript:void(0);");
                $(v).attr("onclick", "adssys(" + AdsSys + ",0,'" + C_PAGE_NAME + "');window.open('" + data.C_LINKADDRESS + "');");
            });
        })
    </script>
      


