    <script type="text/javascript" >
        var objLogs = {
            sID: 18,
            mtID: 1801,
            parameterValues: escapeSpecChar(''),
            hdaModel: 3,
            hdaID: '4861',
            hdaName: 'Developer',
            hdaLocation: '',
            hdaPrj: '',
            hdaDev: '',
            hdaPrice: '',
            hdaType: 'Info',
