    <script>
        function adssys(id, url, page) {
            $.getJSON('https://ipapi.co/json/', function (data) {
                ip = (data.ip);
                $.post('/adssystem/api/AdsLogAPI', { C_Word: id, RequestedSource: 'D', RequestedIP: ip, UserAgent: HDAobjLogs.userCode, C_Pagename: page }, function (data) {
                    if (data == '' || data == null) {
                        //console.log(data);
                    }
                });
            });


        }


        $.each($('a[href*="/adssystem/UpdateClicks/"]'), function (i, v) {
            var AdsSys = ($(v).attr("href").replace('/adssystem/UpdateClicks/', ''));
            $.get('/adssystem/api/AdsLogAPI', { C_Word: AdsSys }, function (data) {
                var path2 = '/d/apthailand';
                var path3 = 'd';
                var C_PAGE_NAME = data.C_PAGE_NAME;
                C_PAGE_NAME = (path3 == 'review' ? 'V9_REVIEW' : C_PAGE_NAME);
                C_PAGE_NAME = (path2 == '/home' ? 'V9_HOME' : C_PAGE_NAME);
                C_PAGE_NAME = (path2 == '/condo' ? 'V9_CONDO' : C_PAGE_NAME);
                C_PAGE_NAME = (path3 == 'hometips' ? 'V8_HOMETIPS' : C_PAGE_NAME);
                C_PAGE_NAME = (path3 == 'tv' ? 'V9_TV' : C_PAGE_NAME);
                $(v).attr("href", "javascript:void(0);");
                $(v).attr("onclick", "adssys(" + AdsSys + ",0,'" + C_PAGE_NAME + "');window.open('" + data.C_LINKADDRESS + "');");
            });
        })
    </script>
      


          <input type="hidden" id="Amode" value="0" />
          <input type="hidden" id="Aid" value="0" />
          <input type="hidden" id="afav" />
          <input type="hidden" id="sfav" />
    
    <script>


        setTimeout(function () {
            var projnext = $(".projnext").attr('onclick');
            var projfavi = $(".projfavi").attr('onclick');
            var projrec = $(".projrec").attr('onclick');
            var sha01 = $(".sha01").attr('onclick');
            var shl01 = $(".shl01").attr('onclick');

            if (projnext != undefined && projnext != "" && projnext != null) {
                $(".projnext3").css('display', 'inline-block');
            } else {
                $('.sentmail03').hide();
            }
            if (projfavi != undefined && projfavi != "" && projfavi != null) {
                $(".projfavi3").css('display', 'inline-block');
            } else {
                $('.sentmail03').hide();
            }
            if (projrec != undefined && projrec != "" && projrec != null) {
                $(".projrec3").css('display', 'inline-block');
            } else {
                $('.sentmail03').hide();
            }
            if (sha01 != undefined && sha01 != "" && sha01 != null) {
                $(".sha03").css('display', 'inline-block');
            }
            //console.log(projnext, projfavi, sha01, shl01);
            if (shl01 != undefined && shl01 != "" && shl01 != null) {
                $(".shl03").css('display', 'inline-block');
            }

        }, 3000);
        setTimeout(function () {
            var projnext = $(".projnext").attr('onclick');
            var projfavi = $(".projfavi").attr('onclick');
            var projrec = $(".projrec").attr('onclick');
            var sha01 = $(".sha01").attr('onclick');
            var shl01 = $(".shl01").attr('onclick');

