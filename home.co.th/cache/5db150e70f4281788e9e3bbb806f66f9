    <script>
        var clicklink = function (t) {
            $('html, body').animate({
                scrollTop: $('a[name="'+t+'"]').offset().top
            }, 500);
        }


        $(function () {

            $('.branddefault').click(function () {
                $('.d_icon').html('');
                $('[id^="pl"]').hide().fadeIn();
                $('#passproject').show();
                $('.tytitle').show();
                $('.tysc').show();
            });

            $('.bandlistitem').on("click", ".brandselect", function () {
                //$('.shownextpro').hide();
                var x = $(this).attr("data-value");
                var y = $('a[data-value="'+x+'"]').attr('data-imguri');
                console.log(x, y);
                $('.d_icon').html('<img src="' + y + '" style="height: 35px;">');
                $('[id^="pl"],#passproject').hide();
                $("div[data-brandid=" + x + "]").fadeIn();
                $('.tytitle').hide();
                //$('.tysc').hide();
                $.each($('[id^="pl"],#passproject'), function (i, v) {
                    if ($(v).css('display') == 'block') {
                        $('h1[ty="' + $(v).attr('ty2') + '"]').show();
                        //$('a[onclick="clicklink(\'' + $(v).attr('ty2') + '\')"]').show();
                    }
                })
            });

            //$('.shownextpro').click(function () {
            //    $(this).parent().hide();
            //    var divx = $(this).attr("data-val");
            //    $('[id^="' + divx + '"]').fadeIn().next().fadeIn();
            //});
            $('.shownextpro').click(function () {
                $(this).hide();
                //$(this).parent().hide();
                var divx = $(this).attr("data-val");
                $('[id^="' + divx + '"]').fadeIn().next().fadeIn();
                //$('.' + divx).fadeIn(1000)
            });

            $('.sclltotype').click(function (e) {
                e.preventDefault();
                var name = $(this).attr('href').substr(1);
                var pos = $('a[name=' + name + ']').offset();
                $('body').animate({ scrollTop: pos.top - 100 }, '4000');
               
            });

            //$(".bandlistitem").owlCarousel({
            //    margin: 10,
            //    loop: true,
            //    navText:"  ",
            //    autoWidth: true,
            //});
            $(".bandlistitem").slick({
                dots: true,
