            $('.btn_submit_sent').click(function () {
                    //$('#send-mail-developer').show();
                    //e.preventDefault();
                    var curUrl = window.location.href;
                    var d = new Date();
                    

                    var name = false;
                    var phone = false;
                    var mail = false;
                    var msg = false;
                    var checkcode = false;



                    if ($('#fullname-field').val().length <= 0) {
                        $('#fullname-field').removeClass("has-success").addClass("has-error");
                        name = false;
                    } else {
                        $('#fullname-field').removeClass("has-error").addClass("has-success");
                        name = true;
                    }

                    if ($('#phone-field').val().length <= 0) {
                        $('#phone-field').removeClass("has-success").addClass("has-error");
                        phone = false;
                    } else {
                        $('#phone-field').removeClass("has-error").addClass("has-success");
                        phone = true;
                    }

                    if ($('#email-field').val().length <= 0) {
                        $('#email-field').removeClass("has-success").addClass("has-error");
                        mail = false;
                    } else {
                        $('#email-field').removeClass("has-error").addClass("has-success");
                        mail = true;
                    }

                    if ($('#message-field').val().length <= 0) {
                        $('#message-field').removeClass("has-success").addClass("has-error");
                        msg = false;
                    } else {
                        $('#message-field').removeClass("has-error").addClass("has-success");
                        msg = true;
                    }

                    if (name == true && phone == true && mail == true && msg == true) {
                        if ($('#imgcode').val().length <= 0) {
                            $('#imgcode').removeClass("has-success").addClass("has-error");
                            $('.statusmail').html('<p class="text-danger">กรุณากรอกข้อมูลให้ครบถ้วน...</p>');

                        } else {
                            $('#imgcode').removeClass("has-error").addClass("has-success");
                            $('#send-mail-developer').html('กำลังส่งข้อมูล...');
                            var urlchecl = "/CaptchaImage/check?prefix=contact&code=" + $('#imgcode').val();

                            $.get(urlchecl, function (obj) {
                                if (obj == "false") {
                                    $('#imgcode').removeClass("has-success").addClass("has-error");
                                    $('.statusmail').html('<p class="text-danger">กรุณากรอกข้อมูลให้ครบถ้วน...</p>');
                                } else {

                                    var putObj = {

                                        MemberId: $('input[name="memberid"').val(),
                                        Message: $('#message-field').val(),
                                        DeveloperId: $('input[name="developerId"').val(),
                                        Email: $('#email-field').val(),
                                        Phone: $('#phone-field').val()

                                    }

                                    //$.ajax({
                                    //    url: '/member/api/inboxapi',
                                    //    type: 'PUT',
                                    //    data: putObj,
                                    //    success: function (data) {

                                    //    }
                                    //});
                                    var getmailPO2 = {
                                        MODE: '1',
                                        N_IDCODE: $('input[name="developerId"').val(),
                                        C_PAGE: window.location.href,
                                        C_EMAIL: $('#email-field').val(),
                                        C_NAME: $('#fullname-field').val(),
                                        DATA_GROUP: '3',
                                        C_DESC: $('#message-field').val(),
                                        C_TEL: $('#phone-field').val(),
                                        C_DEVICE_CODE: $('#DEVICE_CODE').val(),
                                        C_COOKIE_CODE: $('#hdack').val()
                                    }
                                    //$.ajax({
                                    //    type: "POST",
                                    //    url: "/member/api/AddEmailAPI",
                                    //    data: getmailPO2,
                                    //    success: function (data2) {
                                    //        if (data2 == 1) {

                                    //        } else {
                                    //            alert('error');
                                    //        }
                                    //    }
                                    //})
                                    var MailPO2 = {
                                        TO: $('#email-field').val(),
                                        CC: '',
                                        BCC: 'member@home.co.th',
                                        From: 'member@home.co.th',
                                        Subject: '',
                                        Message: window.location.href + '|บริษัท แสนสิริ จำกัด (มหาชน)|' + $('#fullname-field').val() + '|' + $('#phone-field').val() + '|' + $('#message-field').val() + '|' + 'https://www.home.co.th/images/img_v/img_Developer/20160615-134653794-แสนสิริ.jpg' + '',
                                ProjNoList: '2895',
                                        groupType: window.location.href,
                                        C_DEVICE_CODE: 'D',
                                        C_IPADD: '',
                                        TOEMAIL: $('input[name="developerMail"]').val(),
                                        C_GETBACK: '',
                                        userCode: '',
                                        C_NAME: $('#fullname-field').val(),
                                        C_DESC: $('#message-field').val(),
                                        C_TEL: $('#phone-field').val(),
                                        //C_COOKIE_CODE: gethdaCookie("hdaUser"),
