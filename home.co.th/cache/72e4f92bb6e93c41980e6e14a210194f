        <script>
            
            //setTimeout(function () {
            //    $('#Modalloading').modal('show');
            //    //$('#Modalloading').modal('hide');
            //    setTimeout(function () {
            //        $('#Modalloading').modal('hide');
            //    }, 7000);
            //}, 1000);
            
  </script>

  
        <script>
            $('.getinfologin2').click(function () {

                var MailPO = {
                    TO: '',
                    CC: '',
                    BCC: 'member@home.co.th',
                    From: 'member@home.co.th',
                    Subject: 'รับข้อมูลข่าวสาร',
                    Message: '',
                    ProjNoList: '',
                    groupType: window.location.href,
                    C_DEVICE_CODE: 'D',
                    C_IPADD: '',
                    TOEMAIL: '',
                    C_GETBACK: '',
                    userCode: '',
                    C_NAME: '',
                    C_DESC: '',
                    C_TEL: '',
                    C_COOKIE_CODE: hdaUserAccount(),
                    C_DATA_LIST: '{"info":"' + getHomeCookie('info') + '","near":"' + getHomeCookie('infog') + '","rec":"' + getHomeCookie('inforec') + '"}'
                }
                if ($('#getmailPO2_MODE').val() == '1') {
                    MailPO.TO = $('#getmailPO2_C_EMAIL').val();
                    MailPO.ProjNoList = $('#getmailPO2_N_IDCODE').val();
                    MailPO.groupType = $('#getmailPO2_MODE').val();
                } else if ($('#getmailPO2_MODE').val() == '4') {
                    MailPO.TO = $('#getmailPO2_C_EMAIL').val();
                    MailPO.ProjNoList = $('#getmailPO2_List_N_IDCODE').val();
                    MailPO.groupType = $('#getmailPO2_MODE').val();
                } else if ($('#getmailPO2_MODE').val() == '5') {
                    MailPO.TO = $('.getsetmailfav').val();
                    MailPO.groupType = $('#getmailPO2_MODE').val();
                    var arrfav = [];
                    $.each($('.favlike[chk="1"]'), function (i, v) {
                        arrfav.push($(v).attr('projno'));
                    })
                    MailPO.ProjNoList = $('.sentmail03').attr('projsent');
                }
                HDASendMail(MailPO.groupType + '|รับข้อมูลโครงการ');
                var v = grecaptcha.getResponse();
                if (v != '') {
                    $('#sentmaildata').modal('toggle');
                    $('.favlike').attr('chk', '0');
                    $('.sentmail03').attr('projsent', '');
                    $('.sentmail-num').html('0');
                    $('.sentmail-noti').hide();
                    countfavlike();
                    $('#Modalloading').modal('show');
                    $.getJSON('https://ipapi.co/json/', function (data) {
                        ip = (data.ip);
                        MailPO.C_IPADD = ip;
                        MailPO.C_COOKIE_CODE = hdaUserAccount();
                        MailPO.userCode = window.location.href;

                        $.ajax({
                            type: "POST",
                            url: "/member/api/SentMailSystemAPI",
                            data: MailPO,
                            success: function (data2) {
                                alert('ส่งเมล์สำเร็จ');
                                $('#Modalloading').modal('hide');
                            }
                        })
                    })
                } else {
                    alert('Click CAPTCHA');
                }

            })
                </script>

