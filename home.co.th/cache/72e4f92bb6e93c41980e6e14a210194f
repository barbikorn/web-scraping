        var getusernameHDA = function () {

            var data = ('') != '' ? ('') : '';
            return data
        }
         var HDAobjLogs = {};
         if (typeof (objLogs) !== 'undefined') {
             var objUser = {
                 userCode: hdaUserAccount(),
                 userType: 'CK',
                 userName: '',
                 requestedDevice: 'D'
             };
             HDAobjLogs = $.extend(objLogs, objUser);
             hdaPostData(HDAobjLogs);
             $('#hdack').val(HDAobjLogs.userCode);
         }
         var HDAClick = function (t, n, l, m, d) {

             if (t != '' && t != null) { HDAobjLogs['hdaType'] = t; }
             if (n != '' && n != null) { HDAobjLogs['hdaName'] = n; }
             if (l != '' && l != null) { HDAobjLogs['hdaLocation'] = l; }
             if (m != '' && m != null) { HDAobjLogs['hdaModel'] = m; }
             if (d != '' && d != null) { HDAobjLogs['hdaDesc'] = escapeSpecChar(d); }
             HDAobjLogs['userName'] = getusernameHDA();
             hdaPostData(HDAobjLogs);
         }
         var HDAClickPage = function (m, n, t, d) {

             if (m != '' && m != null) { HDAobjLogs['mtID'] = m; }
             if (n != '' && n != null) { HDAobjLogs['hdaName'] = n; }
             if (t != '' && t != null) { HDAobjLogs['hdaType'] = t; }
             if (d != '' && d != null) { HDAobjLogs['hdaDesc'] = escapeSpecChar(d); }
             HDAobjLogs['userName'] = getusernameHDA();
             ////console.log(HDAobjLogs);
             hdaPostData(HDAobjLogs);
         }
         var HDASendMail = function (p) {
             HDAobjLogs['hdaType'] = 'sendmail';
             if (p != '' && p != null) { HDAobjLogs['parameterValues'] = escapeSpecChar(p); }
             HDAobjLogs['userName'] = getusernameHDA();
