      <script>
          if (getprojCookie() != '' || getprojgCookie() != '' || getprojrecCookie() != '0') {
              $('#scrollTop4').show();
          }
          function scrollTotop(element) {
              element.animate({ bottom: '130px' });
          }
          function scrollTodown(element) {
              element.animate({ bottom: '-600px' });
          }

          $('#scrollTop4').click(function () {
              if ($('#scrollTop5').attr('class') == 'closereviewproj5') {
                  scrollTodown($('#scrollTop5'));
                  $('#scrollTop5').removeClass('closereviewproj5');
                  $('.closefav').hide();
              } else {
                  scrollTotop($('#scrollTop5'));
                  $('#scrollTop5').addClass('closereviewproj5');
                  $('.closefav').show();
                  HDAClick('act-btn-recc', '', '', '', '');
              }
          })
      </script>
            <script>
                ////console.log({ a: '', s: '' });
                $.post("/member/api/CookAreaAPI", { a: '', s: '' }, function (d, s) {
                    ////console.log(d);
                    $.get("/review/api/ShAreaAPI", d, function (d, s) {
                        ////console.log(d);
                        var htmltxt = "";
                        var htmltxt1 = "";
                        var htmltxt2 = "";
                        if (d != null) {

                            $.each(d.subarea, function (i, v) {
                                htmltxt1 += '<tr style="border-bottom: #ddd 1px solid;"><td style="width: 50%;">' + v.SubAreaName + '</td><td>';
                                if (v.homecount > 0) {
                                    htmltxt1 += '<a href="/home/' + v.aurl + '/' + v.surl + '" target="_blank" ><img src="https://www.home.co.th/Images/Icon/icon-12-30.png" style="width: 25px;" alt="บ้านเดี่ยว">' + v.homecount + '</a>';
                                }
                                htmltxt1 += '</td><td>';
                                if (v.condocount > 0) {
                                    htmltxt1 += '<a href="/condo/' + v.aurl + '/' + v.surl + '" target="_blank" ><img src="https://www.home.co.th/Images/Icon/icon-13-30.png" style="width: 25px;" alt="คอนโดมิเนียม">' + v.condocount + '</a>';
                                }

                                htmltxt1 += '</td></tr>';
                            })
                            $.each(d.landmark, function (i, v) {
                                htmltxt2 += '<div style="float: left;padding: 0;"><a  href="/l/' + v.PoiUrl_Keyword + '" target="_blank"  style="margin-right: 5px;line-height: 35px;color: #4e64b7;font-size: 1.4rem;background: rgba(78,100,183,.1);padding: 5px 10px;">' + v.PoiUrl_Keyword.replace(/-/g, '') + '</a></div>';
                            })
                            htmltxt2 += '<div style="float: left;padding: 0;width: 100%;"></div>';
                            $.each(d.tag, function (i, v) {
                                htmltxt2 += '<div style="float: left;padding: 0;"><a  href="' + v.PoiUrl_Keyword + '" target="_blank"  style="margin-right: 5px;line-height: 35px;color: #4e64b7;font-size: 1.4rem;padding: 5px 10px;border-radius: 60px;border: solid 1px #BBB;">' + v.PoiName.replace(/-/g, '') + '</a></div>';
                            })
                            if (d.area != null) {
                                htmltxt += '<div class="sha01" onclick="closemanu(3)"><label style="width: 70%;line-height: 20px;font-weight: 300;">โครงการ' + (d.area.AreaNo >= 31 && d.area.AreaNo <= 36 ? '' : 'ย่าน') + ' ' + d.area.AreaName + '</label></div>';
                                htmltxt += '<table class="sha02" style="margin-left: 20px;width: 100%;line-height: 40px;overflow: hidden;display:none;">'
                                htmltxt += htmltxt1 + '</table>';
                                htmltxt += '<div class="shl01" onclick="closemanu(4)">โครงการใกล้ Landmark-Tag</div><div class="shl02" style="display:none;">'
                                htmltxt += htmltxt2 + '</div>';
                            }
                        }
                        $('.shlist').html(htmltxt);
                    })
                });
              </script>
      <script>
          var projcookN = '';
          $.get('/api/ProjectAPI', { "Proj": getprojCookie().toString() }, function (d, s) {
              if (d != null && d.length > 0) {
                  var txthtml = '<div class="projnext" onclick="closemanu(1)">โครงการที่ดูล่าสุด</div><div class="projnext2" style="background: #fff;display:none;">';

                  var favproj = (d[0].MapFileName == null ? '' : d[0].MapFileName);
                  $.each(d, function (i, v) {
                      //console.log(v);
                      if (i == 0) {
                          projcookN = v.ProjName;
                      }
                      var projtypeicon = '';
                      switch (v.ProjectType) {
                          case 'บ้านเดี่ยว': projtypeicon2 = 'https://www.home.co.th/images/icon/HomePin.png'; break;
                          case 'ทาวน์โฮม': projtypeicon2 = 'https://www.home.co.th/images/icon/TownhomePin.png'; break;
                          case 'บ้านแฝด': projtypeicon2 = 'https://www.home.co.th/images/icon/HometwinPin.png'; break;
                          case 'คอนโดมิเนียม': projtypeicon2 = 'https://www.home.co.th/images/icon/CondoPin.png'; break;
                          case 'โฮมออฟฟิศ': projtypeicon2 = 'https://www.home.co.th/images/icon/OfficePin.png'; break;
                          default: break;
                      }
                      var projtypeicon01 = (v.ProjGroup == 'คอนโด' ? 'condo' : 'home');
                      txthtml += '<article class="item clearfix">' +
                                      '<div class="image-thumbnail" style="float: left;margin-right: 20px;">' +
                                          '<a href="/' + projtypeicon01 + '/i/' + v.ProjNo + '">' +
                                              '<img alt="' + v.ProjName + '" style="width: 115px;height: 70px;object-fit: contain;display: inline;" src="https://www.home.co.th/' + v.Thumb_IMG + '">' +
                                          '</a>' +
                                          '</div>' +
                                          '<span style="display: grid;">' +
                                          '<h2 class="title" style="color: #333;font-size: 1.4rem;font-weight: 300;line-height: 1.6;margin-top: 0;margin-bottom: 0px;">' +
                                          '<a href="/' + projtypeicon01 + '/i/' + v.ProjNo + '">' + v.ProjName + ' </a>' +
                                      '</h2>' +
                                      '<h2 class="title" style="color: #333;font-size: 1.3rem;font-weight: 300;line-height: 1;margin-top: 0;margin-bottom: 5px;padding-bottom: 5px;border-bottom: solid 0.5px #AAA;">' +
                                          '' + (v.Marketing_Keyword.length > 40 ? v.Marketing_Keyword.substring(0, 40) + '...' : v.Marketing_Keyword) + '' +
                                      '</h2>' +
