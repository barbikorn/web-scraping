<script>
    $(function () {
        $('#coverpage').modal();
        $('#coverpage').on("click", function (e) {
            $('#coverpage').modal('hide');
        });

        var locationmapdiv = $(".location-map");
        var offset = locationmapdiv.offset();
        if (offset != undefined) {
            $(window).scroll(function () {
                if ($(this).scrollTop() > 250) {
                    $('#scrollTop').fadeIn('slow');
                    if ($(this).scrollTop() >= offset.top - 600 && $(this).scrollTop() <= offset.top + 300) {
                        $('#scrollTop2').fadeOut('slow');
                    } else {
                        $('#scrollTop2').fadeIn('slow');
                    }
                } else {
                    $('#scrollTop').fadeOut('slow'); $('#scrollTop2').fadeOut('slow');
                }
            });
            $('#scrollTop').click(function (e) { e.preventDefault(); $('html, body').animate({ scrollTop: 0 }, 800); });
        } else {
            $(window).scroll(function () {
                if ($(this).scrollTop() > 250) {
                    $('#scrollTop').fadeIn('slow'); $('#scrollTop2').fadeIn('slow');
                } else {
                    $('#scrollTop').fadeOut('slow'); $('#scrollTop2').fadeOut('slow');
                }
            });
            $('#scrollTop').click(function (e) { e.preventDefault(); $('html, body').animate({ scrollTop: 0 }, 800); });
        }


    });
 </script>




 <script>
     $.ajax({
         type: "GET",
         url: "/member/api/AddEmailAPI",
         success: function (data3) {
             ////console.log(data3);
             $('.getinformationname').val(data3.C_NAME);
             $('.getinformationemail').val(data3.C_EMAIL);
             $('#email-field').val(data3.C_EMAIL);
             $('#fullname-field').val(data3.C_NAME);
             $('.getmaillogin').val(data3.C_EMAIL);
             if (data3.C_EMAIL != '') {
                 $('.getinfologin').removeAttr('disabled');
                 $('.getinformation').removeAttr('disabled');
                 $('#callmailccdata').val(data3.C_EMAIL);
             }
         }
     })
</script>


  
