<script>
    var addFav = function (i) {
        if (i == 0) {
            $("#modalLogin").modal();
            $('a[onclick="addFav(' + i + ')"]').removeClass();
            $('a[onclick="addFav(' + i + ')"]').addClass('btn btn_hero btn_bookmark');
            setTimeout(function () {
                $('a[onclick="addFav(' + i + ')"]').removeClass();
                $('a[onclick="addFav(' + i + ')"]').addClass('btn btn_hero btn_bookmark');
            }, 1000)
        }
    }
    </script>





