                      switch (v2.ProjectType) {
                          case 'บ้านเดี่ยว': projtypeicon2 = 'https://www.home.co.th/images/icon/HomePin.png'; break;
                          case 'ทาวน์โฮม': projtypeicon2 = 'https://www.home.co.th/images/icon/TownhomePin.png'; break;
                          case 'บ้านแฝด': projtypeicon2 = 'https://www.home.co.th/images/icon/HometwinPin.png'; break;
                          case 'คอนโดมิเนียม': projtypeicon2 = 'https://www.home.co.th/images/icon/CondoPin.png'; break;
                          case 'โฮมออฟฟิศ': projtypeicon2 = 'https://www.home.co.th/images/icon/OfficePin.png'; break;
                          default: break;
                      }
                      var projtypeicon02 = (v2.ProjGroup == 'คอนโด' ? 'condo' : 'home');
                      txthtml2 += '<article class="item clearfix">' +
                                      '<div class="image-thumbnail" style="float: left;margin-right: 20px;">' +
                                          '<a href="/' + projtypeicon02 + '/i/' + v2.ProjNo + '">' +
                                              '<img alt="' + v2.ProjName + '" style="width: 115px;height: 70px;object-fit: contain;display: inline;" src="https://www.home.co.th/' + v2.Thumb_IMG + '">' +
                                          '</a>' +
                                          '</div>' +
                                          '<span style="display: grid;">' +
                                          '<h2 class="title" style="color: #333;font-size: 1.4rem;font-weight: 300;line-height: 1.6;margin-top: 0;margin-bottom: 0px;">' +
                                          '<a href="/' + projtypeicon02 + '/i/' + v2.ProjNo + '">' + v2.ProjName + ' </a>' +
                                      '</h2>' +
                                      '<h2 class="title" style="color: #333;font-size: 1.3rem;font-weight: 300;line-height: 1;margin-top: 0;margin-bottom: 5px;padding-bottom: 5px;border-bottom: solid 0.5px #AAA;">' +
                                           '' + (v2.Marketing_Keyword.length > 40 ? v2.Marketing_Keyword.substring(0, 40) + '...' : v2.Marketing_Keyword) + '' +
                                      '</h2>' +
                                      '<h2 class="title" style="color: #333;font-size: 1.3rem;font-weight: 300;line-height: 1;margin-top: 0;">' +
                                          '' + (v2.HighlightInfo.length > 80 ? v2.HighlightInfo.substring(0, 80) + '...' : v2.HighlightInfo) + '' +
                                      '</h2>' +
                                      '<span class="date" style="font-size: 14px;color: #337ab7;font-weight: bold;margin-top: 5px;"><img src="' + projtypeicon2 + '" style="    width: 25px;margin-top: -10px;margin-right: 10px;" />' + v2.PriceTxt + '' +
                                      '<div class="favlike" chk="0" chkb="0" onclick="favlike(this);" projno="' + v2.ProjNo + '" style="width: 20px;height: 20px;position: absolute;z-index: 2;border-radius: 50%;margin: -20px 20px 0 5px;right: 0;">' +
                                      '<img src="https://www.home.co.th/images/icon/Withlist.png" alt="" style="height: 25px;margin-top: -10px;margin-left: -50px;">' +
                                      '<img src="https://www.home.co.th/images/icon/close-envelope_close.png" alt="" style="width: 25px;margin-left: -10px;position: absolute;top: 1px;">' +
                                      '<img src="https://www.home.co.th/images/icon/close-envelope.png" alt="" style="width: 25px;margin-left: -10px;position: absolute;top: 1px;"></div>' +
                                      '</span>' +
                                      '</span>' +
                                  '</article>';
                  })
                  $('.shprojlist2').html(txthtml2 + '</div>');
              })

          }


          if (getprojfCookie() != '') {
              $.each(getprojfCookie().split(','), function (i, v) {
                  $('article[data-prono="' + v + '"]').addClass('bookmark');
                  setTimeout(function () {
                      //$.each($('.favlike'), function (i, v) {
                      //    //console.log(v,i);
                      //})
                      $('.favlike[projno="' + v + '"]').attr('chkb', '1');
                  }, 3000);

              })
          }
