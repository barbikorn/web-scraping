                    var phone = false;
                    var mail = false;
                    var msg = false;
                    var checkcode = false;



                    if ($('#fullname-field').val().length <= 0) {
                        $('#fullname-field').removeClass("has-success").addClass("has-error");
                        name = false;
                    } else {
                        $('#fullname-field').removeClass("has-error").addClass("has-success");
                        name = true;
                    }

                    if ($('#phone-field').val().length <= 0) {
                        $('#phone-field').removeClass("has-success").addClass("has-error");
                        phone = false;
                    } else {
                        $('#phone-field').removeClass("has-error").addClass("has-success");
                        phone = true;
                    }

                    if ($('#email-field').val().length <= 0) {
                        $('#email-field').removeClass("has-success").addClass("has-error");
                        mail = false;
                    } else {
                        $('#email-field').removeClass("has-error").addClass("has-success");
                        mail = true;
                    }

                    if ($('#message-field').val().length <= 0) {
                        $('#message-field').removeClass("has-success").addClass("has-error");
                        msg = false;
