                                    HDAClick('inquiry', '', '', '3', '');
                                    HDASendMail('3|สอบถามข้อมูลบริษัท|' +
                                   $('#email-field').val() + '|' +
                                   $('#phone-field').val() + '|' +
                                   $('#fullname-field').val() + '|' +
                                   $('#message-field').val()
                                   ); $('#Modalloading').modal('show');
                                    $.getJSON('https://ipapi.co/json/', function (data) {
                                        ip = (data.ip);
                                        MailPO2.C_IPADD = ip;
                                        MailPO2.userCode = window.location.href;
                                        MailPO2.C_COOKIE_CODE = hdaUserAccount();
                                        $.ajax({
                                            type: "POST",
                                            url: "/member/api/SentMailSystemForDevAPI",
                                            data: MailPO2,
                                            success: function (data2) {
                                                console.log(data2);
                                                alert('ส่งเมล์สำเร็จ'); $('#Modalloading').modal('hide');
                                                location.reload();
                                            }
                                        })
                                    })
                                }
                            });
                        }

                    } else {
                        $('#send-mail-developer').show();
                        $('.statusmail').html('<p class="text-danger">กรุณากรอกข้อมูลให้ครบถ้วน...</p>');
                    }


                });


            });



    </script>


