                          case 'บ้านเดี่ยว': projtypeicon2 = 'https://www.home.co.th/images/icon/HomePin.png'; break;
                          case 'ทาวน์โฮม': projtypeicon2 = 'https://www.home.co.th/images/icon/TownhomePin.png'; break;
