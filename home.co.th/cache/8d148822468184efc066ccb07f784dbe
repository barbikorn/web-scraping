        var favlike = function (e) {
            var chk = $(e).attr('chk');
            var proglike = $(e).attr('projno');
            if (chk == 1) {
                $('.favlike[projno="' + proglike + '"]').attr('chk', '0');
                countfavlike();
            }
            else if (chk == 0) {
                $('.favlike[projno="' + proglike + '"]').attr('chk', '1');
                countfavlike();
            }

        }
        var countfavlike = function () {
            var dataNo = '0';
            $.each($('.favlike[chk="1"]'), function (i, v) {
                dataNo = dataNo.replace($(v).attr('projno') + ',', '');
                dataNo = $(v).attr('projno') + ',' + dataNo;
            })
            var datanoti = (dataNo.split(',').length - 1);
            $('.sentmail-num').html(datanoti);
            if (datanoti > 0) {
                $('.sentmail-noti').show();
                $('.sentmail03').attr('projsent', dataNo.replace(',0', ''));
                //$('.sentmail03 img').attr('onclick', 'closemanu(10)');
                $('.getsetmailfav').prop("disabled", false);
                $('.getsetmailfav').attr('placeholder', 'ใส่อีเมลรับข้อมูลโครงการ');
                $('.getsetmailfav').val('');
            } else {
                $('.sentmail-noti').hide();
                $('.sentmail03').attr('projsent', '0');
                //$('.sentmail03 img').attr('onclick', 'closemanu(12)');
                $(".getsetmailfav").prop("disabled", true);
                $('.getsetmailfav').attr('placeholder', 'คลิกเลือกโครงการรับข้อมูล');
                $('.getsetmailfav').val('');

            }
        }
        $('.sentmail-noti').click(function () {
            $('.sentmail03 img').click();
        })

        $('.getsetfav').click(function (e) {
            //var v = grecaptcha.getResponse();
            var t = $('.getsetmailfav').val().split('@');
            var tt = t.length;
            var tt2 = $('.getsetmailfav').val().split('.').length;
            if (tt > 1) {
                if ($('.getsetmailfav').val() != '' && tt > 1 && tt2 > 1 && t[0] != '' && t[1] != '') {
                    //$('#sentmaildatainfo').modal('toggle');
                    $//('#modalLogin2').modal('hide');
                    $('#getmailPO2_MODE').val('5');
                    $('.getsetmailfav').css('border', 'solid 1px red');
                    $('#sentmaildata').modal();

                    grecaptcha.reset();
                }
            }

        })

        var closemanu = function (n) {
            if (n != 10 && n != 11) {
                $(".projnext,.projnext3").attr('onclick', 'closemanu(1)');
                $(".projfavi,.projfavi3").attr('onclick', 'closemanu(2)');
                $(".projrec,.projrec3").attr('onclick', 'closemanu(9)');
                $(".sha01,.sha03").attr('onclick', 'closemanu(3)');
                $(".shl01,.shl03").attr('onclick', 'closemanu(4)');
                $(".shchk").removeClass("shchk");
            }

            if (n == 11) {
                $('.modal-bodysetmailfav').hide();
            }
            if (n == 10) {
                $('.modal-bodysetmailfav').show();
            }


            switch (n) {
                case 1: $('.projnext2').show(1000); $('.projfavi2').hide(1000); $('.projrec2').hide(1000); $('.sha02').hide(1000); $('.shl02').hide(1000); $(".projnext").addClass("shchk"); $('#projfavcook').scrollTop(0); break;
                case 2: $('.projnext2').hide(1000); $('.projfavi2').show(1000); $('.projrec2').hide(1000); $('.sha02').hide(1000); $('.shl02').hide(1000); $(".projfavi").addClass("shchk"); $('#projfavcook').scrollTop(94); break;
                case 3: $('.projnext2').hide(1000); $('.projfavi2').hide(1000); $('.projrec2').hide(1000); $('.sha02').show(1000); $('.shl02').hide(1000); $(".sha01").addClass("shchk"); $('#projfavcook').scrollTop(141); break;
                case 4: $('.projnext2').hide(1000); $('.projfavi2').hide(1000); $('.projrec2').hide(1000); $('.sha02').hide(1000); $('.shl02').show(1000); $(".shl01").addClass("shchk"); $('#projfavcook').scrollTop(188); break;
                case 9: $('.projnext2').hide(1000); $('.projfavi2').hide(1000); $('.projrec2').show(1000); $('.sha02').hide(1000); $('.shl02').hide(1000); $(".projrec").addClass("shchk"); $('#projfavcook').scrollTop(47); break;

                case 10: break;
                case 11: break;

                default: $('.projnext2').hide(1000); $('.projfavi2').hide(1000); $('.projrec2').hide(1000); $('.sha02').hide(1000); $('.shl02').hide(1000); break;
            }
            switch (n) {
                case 1: $(".projnext,.projnext3").attr('onclick', 'closemanu(5)'); HDAClick('recc-info', '', '', '', ''); break;
                case 2: $(".projfavi,.projfavi3").attr('onclick', 'closemanu(6)'); HDAClick('recc-near', '', '', '', ''); break;
                case 3: $(".sha01,.sha03").attr('onclick', 'closemanu(7)'); HDAClick('recc-area', '', '', '', ''); break;
                case 4: $(".shl01,.shl03").attr('onclick', 'closemanu(8)'); HDAClick('recc-landmark', '', '', '', ''); break;
                case 5: $(".projnext,.projnext3").attr('onclick', 'closemanu(1)'); break;
                case 6: $(".projfavi,.projfavi3").attr('onclick', 'closemanu(2)'); break;
                case 7: $(".sha01,.sha03").attr('onclick', 'closemanu(3)'); break;
                case 8: $(".shl01,.shl03").attr('onclick', 'closemanu(4)'); break;
                case 9: $(".projrec,.projrec3").attr('onclick', 'closemanu(12)'); HDAClick('recc-recommend', '', '', '', ''); break;
                case 10:
                    $('.sentmail03 img').attr('onclick', 'closemanu(11)');
                    $('.sentmail03').attr('styleopen', '1');
                    HDAClick('recc-mail', '', '', '', '');
                    $('.sentmail03 img').attr('src', 'https://www.home.co.th/images/icon/mailpop_close.png');
                    $('.sentmail03 img').css('height', '22px');
                    break;
                case 11:
                    $('.sentmail03 img').attr('onclick', 'closemanu(10)');
                    $('.sentmail03').attr('styleopen', '0');
                    $('.sentmail03 img').attr('src', 'https://www.home.co.th/images/icon/mailpop.png');
                    $('.sentmail03 img').css('height', '28px');
                    break;
                case 12: $(".projrec,.projrec3").attr('onclick', 'closemanu(9)'); break;
                default: break;
            }
        }
      </script>
    <!--+++++++++++++++++++++    area End ++++++++++++++++++++++++-->

  </body>
</html>

    