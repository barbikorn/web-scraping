                                  '</article>';

                      favproj = favproj.replace(v.ProjNo + ',', '').replace(v.ProjNo, '');
                      $('.shprojlist').html(txthtml + '</div>');
                  })


              }
          })

          if (getprojrecCookie() != '' && getprojrecCookie() != '0') {
              var txthtml3 = '<div class="projrec shchk" onclick="closemanu(12)">โครงการที่คุณอาจสนใจ</div><div class="projrec2" style="background: #fff;">';
              $.get('/api/ProjectAPI', { "Proj": getprojrecCookie().toString() }, function (d1, s1) {
                  $.each(d1, function (i2, v2) {

                      var projtypeicon2 = '';
                      switch (v2.ProjectType) {
                          case 'บ้านเดี่ยว': projtypeicon2 = 'https://www.home.co.th/images/icon/HomePin.png'; break;
