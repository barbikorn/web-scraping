                                        C_DATA_LIST: '{"info":"' + getHomeCookie('info') + '","near":"' + getHomeCookie('infog') + '","rec":"' + getHomeCookie('inforec') + '"}'
                                    }

                                    HDAClick('inquiry', '', '', '3', '');
                                    HDASendMail('3|สอบถามข้อมูลบริษัท|' +
                                   $('#email-field').val() + '|' +
                                   $('#phone-field').val() + '|' +
                                   $('#fullname-field').val() + '|' +
                                   $('#message-field').val()
                                   ); $('#Modalloading').modal('show');
